# KRX Open API 키 설정
# 발급 사이트: https://openapi.krx.co.kr
KRX_API_KEY=your_krx_api_key_here
# 또는 krx/.env.local 에 KRX_OPENAPI_KEY 사용

# KRX 데이터 수집 (선택)
# KRX_OPENAPI_BASE_URL=https://data-dbg.krx.co.kr/svc/apis
# KRX_API_ENDPOINTS={"kospi_daily":{"url":"..."},"kosdaq_daily":{"url":"..."}}

# 네이버 검색 API (뉴스 수집용)
# 발급: https://developers.naver.com/apps
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# 카카오 REST API (daum 소스용, 웹 검색)
# 발급: https://developers.kakao.com
KAKAO_REST_API_KEY=your_kakao_rest_api_key
